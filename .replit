run = "go run ."
modules = ["go-1.21", "nix"]

[nix]
channel = "stable-24_05"

[gitHubImport]
requiredFiles = [".replit"]

[deployment]
run = ["sh", "-c", "go run ."]
ignorePorts = false
deploymentTarget = "gce"

[[ports]]
